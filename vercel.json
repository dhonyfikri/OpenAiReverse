{
  "version": 2,
  "functions": {
    "dist/*.js": {
      "maxDuration": 60
    },
    "src/*.ts": {
      "maxDuration": 60
    }
  },
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}